#include <iostream>
#include <string>

using namespace std;

int main()
{
    string a, b;
    cin >> a >> b;
    int i = 0;
    while(a[i] != 0 and b[i] != 0)
    {
        ++i;
    }
    if(a[i] == 0 and b[i] != 0) cout << "the second name is longer";
    else if(a[i] != 0 and b[i] == 0) cout << "the first name is longer";
    else cout << "both names have the same amount of symbols";
    return 0;
}
